{
  "name": "selosele2",
  "version": "0.1.0",
  "private": true,
  "homepage": ".",
  "scripts": {
    "start": "npm run serve & \"npm run sass:watch\"",
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "lint": "vue-cli-service lint",
    "sass": "node-sass src/assets/scss/style.scss -o public/css/ -r --output-style compressed",
    "sass:watch": "npm run sass -- -w"
  },
  "dependencies": {
    "@vee-validate/rules": "^4.6.9",
    "@vuepic/vue-datepicker": "^3.5.2",
    "ag-grid-community": "^28.2.0",
    "ag-grid-vue3": "^28.2.0",
    "axios": "^0.27.2",
    "core-js": "^3.8.3",
    "moment": "^2.29.4",
    "sweetalert2": "^11.4.33",
    "vee-validate": "^4.6.9",
    "vue": "^3.2.13",
    "vue-loading-overlay": "6.0",
    "vue-router": "^4.1.5",
    "vuex": "^4.0.2"
  },
  "devDependencies": {
    "@babel/core": "^7.12.16",
    "@babel/eslint-parser": "^7.12.16",
    "@vue/cli-plugin-babel": "~5.0.0",
    "@vue/cli-plugin-eslint": "~5.0.0",
    "@vue/cli-service": "~5.0.0",
    "eslint": "^7.32.0",
    "eslint-plugin-vue": "^8.0.3",
    "node-sass": "^7.0.3",
    "sass-loader": "^13.0.2",
    "vue3-markdown-it": "^1.0.10"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/vue3-essential",
      "eslint:recommended"
    ],
    "parserOptions": {
      "parser": "@babel/eslint-parser"
    },
    "rules": {
      "import/no-named-as-default": 0,
      "import/no-named-as-default-member": 0,
      "import/no-unresolved": 0,
      "import/prefer-default-export": 0,
      "no-underscore-dangle": 0,
      "no-console": 0,
      "no-unused-vars": 0,
      "vue/multi-word-component-names": 0,
      "vue/no-multiple-template-root": 0,
      "vue/no-unused-components": 0
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead",
    "not ie 11"
  ]
}
